<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sk.manage.mapper.UserMapper">




    <select id="selectUserByName" resultType="com.sk.manage.entity.User" parameterType="com.sk.manage.entity.User" >

        select *
        from `tb_user`
        where username = #{username}

    </select>

</mapper>
